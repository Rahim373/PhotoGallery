@{
    ViewBag.Title = "Gallery";
}

<div class="row">
    <div class="col s8">
        <div class=" direcrory-box ">
            <ul class="image-list" id="imageGrid"> </ul>
        </div>
    </div>
    <div class="col s4">
        <div class="card grey lighten-5">
            <div class="card-content">
                <form action="#">
                    <div class="file-field">
                        <div class="btn waves-effect waves-light col l12" id="fileSelect">
                            <span>Select File & Upload</span>
                            <input type="file" multiple accept="image/*">
                        </div>
                    </div>
                </form>
                <div class="space"></div><div class="space"></div>
                <div class="space"></div>
                <div class="progress">
                    <div class="determinate" style="width: 0" id="load"></div>
                </div>
                <div class="space"></div>
                <div class="collection" id="uploadList">
                </div>
            </div>
          
        </div>
    </div>
</div>